<%= link_to :back do %>
  <i class="fa fa-arrow-left" aria-hidden="true"></i>
<% end %>
<h5>elections in which <%= @user.user_name %> is a candidate</h5>
<% if @user.election_data.present? %>
  <div class="container-fluid">
    <% @user.election_data.each do |e| %>
      <% election = e.election %>
      <%= render partial: 'elections',locals: { election: election } %>
    <% end %>
  </div>
<% else %>
  <p style="color:red">No Record Found!!!</p>
<% end %>