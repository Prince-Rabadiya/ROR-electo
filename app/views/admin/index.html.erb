<% if current_user.has_role? :super_admin %>
  <div class="container">
    <table class="table">
      <thead>
        <tr>
          <th scope="col">Id</th> 
          <th scope="col">User Name</th>
          <th scope="col">Email</th>
          <th scope="col">Gender</th>
          <th scope="col">DOB</th>
          <th scope="col">deatils</th>
        </tr>
      </thead>
      <tbody>
        <% @users.each do |user| %>
          <tr>
            <td><%= user.id %></td>
            <td><%= user.user_name %></td>
            <td><%= user.email %></td>
            <td><%= user.gender %></td>
            <td><%= user.birth_date %></td>
            <td><%= link_to "show", show_user_admin_path(user.id) %></td>
          </tr>
        <% end %> 
      </tbody>
    </table>
  <div>
<% end %>